{
	"summary": [
		{
			"title": "Total Apps",
			"value": <%= @apps.length %>
		},
		{
			"title": "Active Apps",
			"value": <%= @active_apps.length %>
		},
		{
			"title": "Total Admins",
			"value": <%= @admins.length %>
		},
		{
			"title": "Total Orders",
			"value": 0
		}
	],
	"users": [
		<% @admins.each do |user| %>
		{
			"id": <%= user.id %>,
			"name": "<%= user.name %>",
			"email": "<%= user.email %>",
			"fbId": <%= user.fb_profile ? user.fb_profile.identifier : 0 %>,
			"fbPages": <%= user.fb_pages.length %>,
			"totalApps": <%= user.applications.length %>,
			"createdAt": "<%= user.created_at %>",
			"apps": [
				<% user.applications.each do |app| %>
				{
					"id": <%= app.id %>,
					"checksum": "<%= app.checksum %>",
					"title": "<%= app.title %>",
					"application_type": "<%= app.application_type %>",
					"status": "<%= app.status %>",
					"canvasId": "<%= app.installed? ? app.fb_application.canvas_id : 'null' %>",
					"fbPageId": <%= app.fb_page ? app.fb_page.id : 'null' %>,
					"fbPageIdentifier": <%= app.fb_page ? app.fb_page.identifier : 'null' %>,
					"users": <%= app.fb_users.length %>	
				}<%=',' unless app == user.applications.last%>
				<% end %>
			]
		}<%=',' unless user == @admins.last%>
		<% end %>
	],
	"apps": [
		<% @apps.each do |app| %>
		{
			"id": <%= app.id %>,
			"checksum": "<%= app.checksum %>",
			"title": "<%= app.title %>",
			"application_type": "<%= app.application_type %>",
			"status": "<%= app.status %>",
			"canvasId": "<%= app.installed? ? app.fb_application.canvas_id : nil %>",
			"fbPageId": <%= app.fb_page ? app.fb_page.id : 'null' %>,
			"fbPageIdentifier": <%= app.fb_page ? app.fb_page.identifier : 'null' %>,
			"users": <%= app.fb_users.length %>	
		}<%=',' unless app == @apps.last%>
		<% end %>
	],
	"fbPages": [
		<% @pages.each do |page| %>
		{
			"id": <%= page.id %>,
			"fbIdentifier": "<%= page.identifier %>",
			"name": "<%= page.name %>"
		}<%=',' unless page == @pages.last%>
		<% end %>
	],
	"fbApps": [
		<% @fb_apps.each do |fb_app| %>
		{
			"id": <%= fb_app.id %>,
			"name": "<%= fb_app.name %>",
			"applicationId": "<%= fb_app.app_id %>",
			"secretCode": "<%= fb_app.secret_key %>",
			"type": "<%= fb_app.application_type %>",
			"canvasId": "<%= fb_app.canvas_id %>",
			"namespace": "<%= fb_app.namespace %>"
		}<%=',' unless fb_app == @fb_apps.last%>
		<% end %>
	],
	"plans": [
		<% @plans.each do |plan| %>
		{
			"id": <%= plan.id %>,
			"name": "<%= plan.name %>",
			"stripeId": "<%= plan.stripe_id %>",
			"amount": "<%= plan.amount %>",
			"interval": "<%= plan.interval %>",
			"trialPeriodDays": "<%= plan.trial_period_days %>"
		}<%=',' unless plan == @plans.last%>
		<% end %>
	]
}